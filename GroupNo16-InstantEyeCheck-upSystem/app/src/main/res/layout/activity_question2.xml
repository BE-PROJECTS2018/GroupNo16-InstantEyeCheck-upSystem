<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_question3b"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.admin.instanteyecheck_upsystem.question2">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Disease Name and Treatment Advised:"
        android:id="@+id/textView5"
        android:textSize="25dp"
        android:paddingLeft="15dp"
        android:paddingTop="15dp"
        android:textColor="@color/colorPrimary"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You should be sure about your symptoms. "
        android:id="@+id/textView3"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:paddingLeft="15dp"
        android:paddingTop="30dp"

        android:textColor="#000000" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Check your answers again."
        android:id="@+id/textView4"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:paddingLeft="15dp"
        android:paddingTop="30dp"
        android:textColor="#000000" />

</RelativeLayout>
